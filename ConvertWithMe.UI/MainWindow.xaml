<Window
    x:Class="ConvertWithMe.UI.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:views="clr-namespace:ConvertWithMe.UI.Views"
    xmlns:styles="clr-namespace:ConvertWithMe.Material3.Styles;assembly=ConvertWithMe.Material3"
    Title="MainWindow"
    MinWidth="960"
    MinHeight="540"
    Loaded="Window_Loaded"
    mc:Ignorable="d"
    Background="White">
    <Grid>
        <styles:M3_TextField TextFieldType="Filled"
                             Width="300"
                             Height="100"/>
        <styles:M3_Slider
            VerticalAlignment="Bottom" Maximum="100" Value="50"/>
    </Grid>

</Window>
